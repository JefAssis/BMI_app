<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme._1_Projeto" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_400</item>
        <item name="colorOnPrimary">@color/purple_50</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/sanonimous_blue_600</item>
        <item name="colorSecondaryVariant">@color/sanonimous_blue_200</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>

    <style name="Theme.1_Projeto" parent="Base.Theme._1_Projeto" />

    <style name="Title1" parent="Widget.AppCompat.TextView">
        <item name="android:textSize">25sp</item>
        <item name="fontFamily">@font/dm_sans_medium</item>
        <item name="android:textColor">@color/purple_800</item>
        <item name="android:textStyle">bold</item>
</style>


